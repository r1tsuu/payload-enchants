{
  "name": "@payload-enchants/better-localized-fields",
  "private": false,
  "version": "1.1.0",
  "repository": "https://github.com/r1tsuu/payload-enchants",
  "bugs": "https://github.com/r1tsuu/payload-enchants/issues",
  "author": "r1tsuu",
  "type": "module",
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "peerDependencies": {
    "@payloadcms/next": "^3.0.0-beta.24",
    "@payloadcms/translations": "^3.0.0-beta.24",
    "@payloadcms/ui": "^3.0.0-beta.24",
    "payload": "^3.0.0-beta.24",
    "react": "^18.2.0",
    "flatley": "^5.2.0"
  },
  "devDependencies": {
    "@payloadcms/next": "3.0.0-beta.24",
    "@payloadcms/translations": "3.0.0-beta.24",
    "@payloadcms/ui": "3.0.0-beta.24",
    "@types/react": "18.2.64",
    "payload": "3.0.0-beta.24",
    "react": "18.2.0"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ]
}
